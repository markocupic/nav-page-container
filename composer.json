{
    "name": "markocupic/nav-page-container",
    "description": "Contao plugin, add the 'not-clickable-page-container' css class to nav items.",
    "keywords": [
        "contao",
        "nav-page-container",
        "navigation",
        "tl_page"
    ],
    "type": "contao-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Cupic",
            "email": "m.cupic@gmx.ch",
            "homepage": "http://4ae-racing-team.ch",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/markocupic/nav-page-container/issues",
        "source": "https://github.com/markocupic/nav-page-container"
    },
    "require": {
        "contao/core-bundle": ">=4.4"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^4.0",
        "contao/test-case": "^4.0",
        "contao/manager-plugin": "^2.3",
        "phpunit/phpunit": "^8.4",
        "symfony/http-client": "4.4.*",
        "symfony/phpunit-bridge": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\NavPageContainerBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/assets",
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\NavPageContainerBundle\\ContaoManager\\Plugin"
    }
}